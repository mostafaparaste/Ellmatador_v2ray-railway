  },
  "inbounds": [
    {
      "port": 8080,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "7b0579b8-4356-4fb1-b652-a50d88c1520d",
            "alterId": 0,
            "email": "user1@v2ray.com",
